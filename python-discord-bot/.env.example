# LLM Provider API Keys
# At least one of these should be set for the bot to function properly
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here

# Discord Bot Configuration
# Required for the Discord bot to connect to your server
DISCORD_BOT_TOKEN=your_discord_bot_token_here
# Optional: Set to restrict commands to a specific server, leave as is for global commands
DISCORD_GUILD_ID=your_discord_server_id_here

# TheraLab Backend
# URL of the TheraLab backend API
API_BASE_URL=http://localhost:3000/api

# Optional Configuration
# LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL 